---
---
[
  {% for post in site.posts %}
    {

      "title"    : "{{ post.title}}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "author"   : "{{ post.author }}",
      "date"     : "{{ post.date | date_to_string}}",
      "content"  : "{{ post.content | strip_html | strip_newlines }}"

    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]